{
  "name": "preact-webcomp-test",
  "version": "1.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress",
    "start": "superstatic build -p ${PORT:-8080} --gzip -c '{\"routes\":{\"**\":\"index.html\"},\"cache_control\":{\"**\":86400}}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdir -p build && ncp src/assets build/assets",
    "test": "eslint src tests/**/*.js"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.3.19",
    "chai": "^3.5.0",
    "core-js": "^2.0.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.5.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "superstatic": "^3.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "decko": "^1.1.3",
    "onsenui": "^2.0.0-beta.6",
    "preact": "^3.0.1",
    "proptypes": "^0.14.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "main": "webpack.config.babel.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c58/preact-webcomp-test.git"
  },
  "bugs": {
    "url": "https://github.com/c58/preact-webcomp-test/issues"
  },
  "homepage": "https://github.com/c58/preact-webcomp-test#readme"
}
